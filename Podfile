pod 'FBSDKShareKit', '~> 4.22.1'
pod 'Google-Mobile-Ads-SDK'


  target 'MyWebTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MyWebUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
